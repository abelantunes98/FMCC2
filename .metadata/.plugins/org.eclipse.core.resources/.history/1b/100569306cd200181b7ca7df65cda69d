package util;

public class Util {
	
	/**
	 * Verifica se um número recebido é ou não primo.
	 * 
	 * @param n
	 * @return
	 */
	
	public static boolean isPrime(long n) {
		boolean result = true;
		for (int i = 2; i < n; i++) {
			if (n % i == 0){
				result = false;
				break;
			}
		}
		return result;
	}
}
